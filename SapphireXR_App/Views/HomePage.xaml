                                                    <Thickness Top="{StaticResource p_2}" Left="0" Right="0" Bottom="{StaticResource p_1}" />
                                                </Label.Padding>
                                            </Label>
                                            <ComboBox HorizontalContentAlignment="Center" Style="{StaticResource theComboBox}" IsEnabled="{Binding Source={x:Static model:PLCConnectionState.Instance}, Path=Online}" Height="26" ItemsSource="{Binding ThrottleValveControlModes, Mode=OneTime}" SelectedItem="{Binding CurrentThrottleValveControlMode, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged">
                                                        <i:InvokeCommandAction Command="{Binding OnThrottleValveModeChangedCommand}" PassEventArgsToCommand="True"/>
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </ComboBox>
                                            <Label Content="Throttle Valve Status" FontFamily="Segoe UI" Foreground="{StaticResource Gray_900}" FontSize="12">
                                                <Label.Padding>
                                                    <Thickness Top="{StaticResource p_2}" Left="0" Right="0" Bottom="{StaticResource p_1}" />
                                                </Label.Padding>
                                            </Label>
                                            <TextBox Text="{Binding ThrottleValveStatus, Mode=OneWay}" IsReadOnly="True" Style="{StaticResource DefaultTextBox}" Height="26"/>
                                        </StackPanel>
                                    </Border>
                                    <Border>
                                        <Border.BorderThickness>
                                            <Thickness Left="{StaticResource p_5}" />
