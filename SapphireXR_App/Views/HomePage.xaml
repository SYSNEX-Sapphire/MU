                                <StackPanel Orientation="Vertical">
                                    <Border>
                                        <Border.BorderThickness>
                                            <Thickness Left="{StaticResource p_5}" />
                                        </Border.BorderThickness>
                                        <Border.Margin>
                                            <Thickness Bottom="{StaticResource p_1}" />
                                        </Border.Margin>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="116"/>
                                                <ColumnDefinition />
                                                <ColumnDefinition Width="116"/>
                                            </Grid.ColumnDefinitions>
                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Vertical">
                                                <Label HorizontalAlignment="Center" Content="Target Val." Foreground="{StaticResource Gray_900}" FontSize="12" FontFamily="Segoe UI" Style="{StaticResource labelOverTextBox}"/>
                                                <TextBox x:Name="tbTargetPress"  Grid.Column="0" Grid.Row="1"  Text="{Binding TargetPress}" IsReadOnly="True" HorizontalAlignment="Stretch" Height="18" Style="{StaticResource DefaultTextBox}"/>
                                            </StackPanel>
                                            <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Vertical">
                                                <Label HorizontalAlignment="Center" Content="Control Val." Foreground="{StaticResource Gray_900}" FontSize="12" FontFamily="Segoe UI" Style="{StaticResource labelOverTextBox}"/>
                                                <TextBox x:Name="tbControlPress"  Grid.Column="2" Grid.Row="1" Text="{Binding ControlPress}" IsReadOnly="True" HorizontalAlignment="Stretch" Height="18" Style="{StaticResource DefaultTextBox}"/>
                                            </StackPanel>
                                        </Grid>
                                    </Border>
                                    <Border>
                                        <Border.BorderThickness>
                                            <Thickness Left="{StaticResource p_5}" />
                                        </Border.BorderThickness>
