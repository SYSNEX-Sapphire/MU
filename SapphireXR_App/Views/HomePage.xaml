                                    <StackPanel.Margin>
                                        <Thickness Bottom="{StaticResource p_1}" />
                                    </StackPanel.Margin>
                                    <Image Source="/Resources/Icons/icon=rotation.png" Width="24" Height="24" />
                                    <Label Foreground="{StaticResource Gray_950}" FontSize="14" HorizontalContentAlignment="Center" FontFamily="Segoe UI">
                                        <Bold>Rotation Controller</Bold>
                                    </Label>
                                </StackPanel>
                                <StackPanel Orientation="Vertical">
                                    <Border>
                                        <Border.BorderThickness>
