                                                        <ScaleTransform ScaleX="0.9" ScaleY="0.9"/>
                                                    </ToggleButton.LayoutTransform>
                                                </ToggleButton>
                                                <Button Grid.Column="2" x:Name="btPumpReset" Command="{Binding VacuumPumpResetCommand}" Height="28" Style="{StaticResource DefaultButton}">
                                                    <StackPanel Orientation="Horizontal">
                                                        <Image Source="/Resources/icons/icon_line_reset.png" Width="12" Height="12"/>
                                                        <Label FontFamily="Segoe UI" FontSize="12" Content="Reset" Foreground="{StaticResource Gray_900}" />
                                                    </StackPanel>
                                                </Button>
                                            </Grid>
                                        </StackPanel>
                                    </Border>
                                </StackPanel>
                            </StackPanel>
                        </Border>                       
                    </Grid>
                    
                    <!-- Susceptor Rotation Current Status -->
                    <Grid Grid.Row="3">
