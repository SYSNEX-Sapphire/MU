                                            </StackPanel>
                                            <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Vertical">
                                                <Label HorizontalAlignment="Center" Content="Control Val." Foreground="{StaticResource Gray_900}" FontSize="12" FontFamily="Segoe UI" Style="{StaticResource labelOverTextBox}"/>
                                                <TextBox x:Name="tbControlRotation"  Grid.Column="2" Grid.Row="1" Text="{Binding ControlRotation}" IsReadOnly="True" HorizontalAlignment="Stretch" Height="18" Style="{StaticResource DefaultTextBox}"/>
                                            </StackPanel>
                                            <StackPanel Grid.Row="0" Grid.Column="4" Orientation="Vertical">
                                                <Label HorizontalAlignment="Center" Content="Current Val." Foreground="{StaticResource Gray_900}" FontSize="12" FontFamily="Segoe UI" Style="{StaticResource labelOverTextBox}"/>
                                                <TextBox x:Name="tbCurrentRotation"  Grid.Column="2" Grid.Row="1" Text="{Binding CurrentRotation}" IsReadOnly="True" HorizontalAlignment="Stretch" Height="18" Style="{StaticResource DefaultTextBox}"/>
                                            </StackPanel>
                                        </Grid>
                                    </Border>
                                    <Border>
                                        <Border.BorderThickness>
                                            <Thickness Left="{StaticResource p_5}" />
                                        </Border.BorderThickness>
                                        <Border.Margin>
                                            <Thickness Top="{StaticResource p_1}" Bottom="{StaticResource p_1}" />
                                        </Border.Margin>
                                        <StackPanel Orientation="Vertical">
                                            <Label Content="Motor Control" FontFamily="Segoe UI" Foreground="{StaticResource Gray_900}" FontSize="12">
                                                <Label.Padding>
                                                    <Thickness Top="0" Left="0" Right="0" Bottom="{StaticResource p_1}" />
                                                </Label.Padding>
                                            </Label>
                                            <TextBox x:Name="btMotorReset" Text="{Binding RotationReset, Mode=OneWay}" Style="{StaticResource DefaultTextBox}" Height="26" IsReadOnly="True"/>
                                        </StackPanel>
                                    </Border>
                                    <Border>
                                        <Border.BorderThickness>
                                            <Thickness Left="{StaticResource p_5}" />
                                        </Border.BorderThickness>
                                        <Border.Margin>
                                            <Thickness Top="{StaticResource p_1}"  />
                                        </Border.Margin>
                                        <StackPanel Orientation="Vertical">
                                            <Label Content="Motor Reset" FontFamily="Segoe UI" Foreground="{StaticResource Gray_900}" FontSize="12">
                                                <Label.Padding>
                                                    <Thickness Top="0" Left="0" Right="0" Bottom="{StaticResource p_1}" />
                                                </Label.Padding>
                                            </Label>
                                            <Button x:Name="btRotorReset" Command="{Binding MotorResetCommand}" Height="28" Style="{StaticResource DefaultButton}">
                                                <StackPanel Orientation="Horizontal">
                                                    <Image Source="/Resources/icons/icon_line_reset.png" Width="12" Height="12"/>
                                                    <Label FontFamily="Segoe UI" FontSize="12" Content="Reset" Foreground="{StaticResource Gray_900}" />
                                                </StackPanel>
                                            </Button>
                                        </StackPanel>
                                    </Border>
                                </StackPanel>
                            </StackPanel>
                        </Border>                      
                    </Grid>
                    <!-- Event Log -->
                    <Grid Grid.Row="4" >
                        <Grid.Margin>
                            <Thickness Top="{StaticResource p_1}" />
                        </Grid.Margin>
                        <StackPanel>
                            <StackPanel.Margin>
                                <Thickness Bottom="0" Left="{StaticResource p_2}" Right="{StaticResource p_2}"/>
                            </StackPanel.Margin>
                            <DockPanel>
                                <DockPanel.Margin>
                                    <Thickness Bottom="{StaticResource p_1}" />
                                </DockPanel.Margin>
                                <Image Source="/Resources/Icons/icon=event.png" Width="24" Height="24" />
                                <Label Foreground="{StaticResource Gray_950}" FontSize="14" HorizontalContentAlignment="Center" FontFamily="Segoe UI">
                                    <Bold>Event Log</Bold>
                                </Label>
                                <Button Width="134" HorizontalAlignment="Right" Style="{StaticResource DefaultButton}" Content="Clear" Command="{Binding ClearEventLogsCommand}"/>
                            </DockPanel>
                            <DataGrid x:Name="dgEventLog" ToolTip="상세 윈도우를 뛰우려면 더블클릭 하세요." VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" AutoGenerateColumns="False" ItemsSource="{Binding Source={x:Static model:EventLogs.Instance}, Path=EventLogList}" Style="{StaticResource EventLog}" Height="215">
                                <DataGrid.Resources>
                                    <Style TargetType="ScrollBar" BasedOn="{StaticResource DefaultScrollbar}" />
                                </DataGrid.Resources>
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="MouseDoubleClick">
                                        <i:InvokeCommandAction Command="{Binding OnDoubleClickedCommand}" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Name" Binding="{Binding Name, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"  ElementStyle="{StaticResource EventLogElement}"/>
                                    <DataGridTextColumn Header="Date" Binding="{Binding Date, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}"  ElementStyle="{StaticResource EventLogElement}"/>
                                    <DataGridTextColumn Header="Message" Binding="{Binding Message, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" ElementStyle="{StaticResource EventLogElement}" MinWidth="97"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </StackPanel>
                    </Grid>
                </Grid>
            </Border>
        </Grid>
    </Grid>
</Page>
