                                                <TextBox x:Name="tbCurrentPress"  Text="{Binding CurrentPress}" IsReadOnly="True" HorizontalAlignment="Stretch" Height="18" Style="{StaticResource DefaultTextBox}"/>
                                            </StackPanel>
                                            <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Vertical">
                                                <Label HorizontalAlignment="Center" Content="Position" Foreground="{StaticResource Gray_900}" FontSize="12" FontFamily="Segoe UI" Style="{StaticResource labelOverTextBox}" />
                                                <TextBox x:Name="tbPosition" Text="{Binding ValvePosition}" IsReadOnly="True" HorizontalAlignment="Stretch" Height="18" Style="{StaticResource DefaultTextBox}"/>
                                            </StackPanel>
                                        </Grid>
                                    </Border>
                                    <Border>
                                        <Border.BorderThickness>
                                            <Thickness Left="{StaticResource p_5}" />
                                        </Border.BorderThickness>
                                        <Border.Margin>
                                            <Thickness Top="{StaticResource p_1}" Bottom="{StaticResource p_1}" />
                                        </Border.Margin>
                                        <StackPanel Orientation="Vertical">
                                            <Label Content="Throttle Valve Mode" FontFamily="Segoe UI" Foreground="{StaticResource Gray_900}" FontSize="12">
