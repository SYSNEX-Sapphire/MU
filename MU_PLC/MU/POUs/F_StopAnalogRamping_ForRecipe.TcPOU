<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4026.11">
  <POU Name="F_StopAnalogRamping_ForRecipe" Id="{894f4883-78ef-41b6-90b5-91f6e5cdbce2}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION F_StopAnalogRamping_ForRecipe
VAR
	i: UINT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[FOR i := 1 TO GVL_CONSTANT.NumAnalogController DO
	GVL_INTERFACE_UI.aAnalogController[i].cvController.input.restart := TRUE;
	GVL_INTERFACE_UI.aAnalogController[i].cvController.input.rampTime := 1; 
	GVL_INTERFACE_UI.aAnalogController[i].cvController.input.targetValue := LREAL_TO_REAL(GVL_INTERFACE_UI.aAnalogController[i].cvController.rControlValue);
END_FOR]]></ST>
    </Implementation>
    <LineIds Name="F_StopAnalogRamping_ForRecipe">
      <LineId Id="1" Count="0" />
      <LineId Id="32" Count="1" />
      <LineId Id="20" Count="0" />
      <LineId Id="5" Count="0" />
    </LineIds>
  </POU>
</TcPlcObject>